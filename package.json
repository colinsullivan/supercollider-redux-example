{
  "name": "supercollider-redux-example",
  "version": "0.0.1",
  "description": "Example for using supercollider-redux.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start_simple": "babel-node simple.js",
    "start_abletonlink": "babel-node abletonlink.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colinsullivan/supercollider-redux-example.git"
  },
  "keywords": [
    "supercollider",
    "redux"
  ],
  "author": "Colin Sullivan <colin [at] colin-sullivan.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colinsullivan/supercollider-redux-example/issues"
  },
  "homepage": "https://github.com/colinsullivan/supercollider-redux-example#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "redux": "^3.6.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": false,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error"
    }
  }
}
